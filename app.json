{
  "expo": {
    "name": "ImportEspana",
    "slug": "ImportEspana",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icons/ios-light.png",
    "scheme": "importespana",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "url": "https://u.expo.dev/3a93fcfd-b8ed-4b00-a6e1-2641bfa41930"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "buildNumber": "3",
      "supportsTablet": true,
      "bundleIdentifier": "com.gio-khimsho.ImportEspana",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": {
        "light": "./assets/images/icons/ios-light.png",
        "dark": "./assets/images/icons/ios-dark.png",
        "tinted": "./assets/images/icons/ios-tinted.png"
      }
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "monochromeImage": "./assets/images/icons/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.giokhimsho.ImportEspana"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/icons/splash-icon-dark.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8296385442547902~3094231182",
          "iosAppId": "ca-app-pub-8296385442547902~3109718643"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3a93fcfd-b8ed-4b00-a6e1-2641bfa41930"
      }
    }
  }
}
